// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema


generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("MYSQL_URL")
}

model Cat {
  email      String   @unique
  catname    String  
  password   String
  imgUrl     String?
  create     DateTime @default(now())
}